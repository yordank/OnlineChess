@model List<SampleChat.Models.Results>

@{
    ViewBag.Title = "ListGames";
}

<div class="container">
    <div class="well">
        <h2>Games Played</h2>

        <table class="table table-striped table-hover">
            <thead>
                <tr>
                    <th>GameID</th>
                    <th>White</th>
                    <th>Black</th>
                    <th>Result(B=Black Win,W=White win,D=Draw)</th>
                </tr>
            </thead>

            <tbody>
                @foreach (var game in Model)
                {
                  
                    <tr>
                   

                    <td>@game.Id</td>
                    <td>@game.WhiteUserName</td>
                    <td>@game.BlackUserName</td>
                    <td>@game.Result</td>


                     </tr>
                }

        </tbody>
    </table>
</div>
</div>
